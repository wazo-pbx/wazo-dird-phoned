<?xml version="1.0" encoding="UTF-8" ?>
<ThomsonPhoneBook>
{% for result in results -%}
 <DirectoryEntry>
  <Name>{{ result.name|e }}</Name>
  <Telephone>Dial:{{ result.number|e }}</Telephone>
 </DirectoryEntry>
{% else -%}
 <DirectoryEntry>
  <Name>{{ gettext('No entries') }}</Name>
  <Telephone></Telephone>
 </DirectoryEntry>
{% endfor -%}
</ThomsonPhoneBook>
