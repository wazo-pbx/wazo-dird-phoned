<?xml version="1.0" encoding="UTF-8" ?>
<AastraIPPhoneTextMenu style="none" destroyOnExit="yes">
{% if offset_previous is not none -%}
<MenuItem>
 <Prompt>{{ gettext('Previous Page') }}</Prompt>
 <URI>{{ xivo_proxy_url|e }}?xivo_user_uuid={{ xivo_user_uuid|e }}&amp;term={{ term|e }}&amp;limit={{ limit|e }}&amp;offset={{ offset_previous|e }}</URI>
</MenuItem>
{% endif -%}

{% for result in results -%}
 <MenuItem>
  <Prompt>{{ result.name|e }}</Prompt>
  <URI>Dial:{{ result.number|e }}</URI>
 </MenuItem>
{% else -%}
 <MenuItem>
  <Prompt>{{ gettext('No entries') }}</Prompt>
  <URI></URI>
 </MenuItem>
{% endfor -%}

{% if offset_next is not none -%}
<MenuItem>
 <Prompt>{{ gettext('Next Page') }}</Prompt>
 <URI>{{ xivo_proxy_url|e }}?xivo_user_uuid={{ xivo_user_uuid|e }}&amp;term={{ term|e }}&amp;limit={{ limit|e }}&amp;offset={{ offset_next|e }}</URI>
</MenuItem>
{% endif -%}
</AastraIPPhoneTextMenu>
